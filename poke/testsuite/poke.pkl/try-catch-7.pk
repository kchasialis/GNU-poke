/* { dg-do run } */

defun foo = (Exception e) int:
  {
   try
     raise e;
   catch if Exception {code = 2 - 1L}
   {
     print ("caught\n");
   }

   return 0;
  }

defun bar = (Exception e) int:
  {
   try
     return foo (e);
   catch
   {
     print ("uncaught\n");
   }

   return 0;
  }

/* { dg-command {  bar (E_no_ios) } } */
/* { dg-command {  bar (E_div_by_zero) } } */
/* { dg-output "uncaught\n0\ncaught\n0" } */
